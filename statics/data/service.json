[
  {
    "_id": "service_mysql",
    "dockerImage": "restcoder/service_mysql:5.7.12",
    "description": "MySQL",
    "version": "5.7.12",
    "url": "mysql://root:root@{{ip}}:{{port}}/mydb",
    "port": 3306,
    "envName": "MYSQL_URL",
    "limits": {
      "memory": "128M"
    },
    "rank": {
      "name": "mysql"
    }
  },
  {
    "_id": "service_mongodb",
    "dockerImage": "restcoder/service_mongodb:3.2.6",
    "description": "MongoDB",
    "version": "3.2.6",
    "url": "mongodb://{{ip}}:{{port}}/mydb",
    "port": 27017,
    "envName": "MONGODB_URL",
    "limits": {
      "memory": "128M"
    },
    "rank": {
      "name": "mongodb"
    }
  },
  {
    "_id": "service_redis",
    "dockerImage": "restcoder/service_redis:3.2.0",
    "description": "Redis",
    "version": "3.2.0",
    "url": "redis://{{ip}}:{{port}}",
    "port": 6379,
    "envName": "REDIS_URL",
    "limits": {
      "memory": "64M"
    },
    "rank": {
      "name": "redis"
    }
  },
  {
    "_id": "service_postgres",
    "dockerImage": "restcoder/service_postgres:9.5.3",
    "description": "Postgres",
    "version": "9.5.3",
    "url": "postgres://postgres:postgres@{{ip}}:{{port}}/postgres",
    "port": 5432,
    "envName": "POSTGRES_URL",
    "limits": {
      "memory": "64M"
    },
    "rank": {
      "name": "postgres"
    }
  }
]