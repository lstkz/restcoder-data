{
  "swagger": "2.0",
  "info": {
    "title": "Starter: Database connection",
    "version": "1.0.0"
  },
  "paths": {
    "/products": {
      "get": {
        "description": "Returns an array of all products.",

        "responses": {
          "200": {
            "description": "Successful response",
            "schema": {
              "type": "array",
              "items": {
                "title": "Product",
                "properties": {
                  "id": {"type": "integer"},
                  "name": {"type": "string"},
                  "quantity": {"type": "integer"}
                }
              }
            }
          }
        }
      }
    }
  }
}